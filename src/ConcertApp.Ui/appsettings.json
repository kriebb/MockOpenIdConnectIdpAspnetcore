{
  "Authentication": {
    "Microsoft": {
      "Prompt": "select_account",
      "UsePkce": true,
      "ResponseMode": "query",
      "ResponseType": "code",
      "Scope": "openid profile email",
      "MapInboundClaims": true,
      "GetClaimsFromUserInfoEndpoint": true,
      "ClaimName": "name"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
